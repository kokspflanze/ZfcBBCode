{
  "name": "kokspflanze/zfc-bbcode",
  "description": "Zendframework 2 Module to convert BBCode to HTML",
  "keywords": [
    "framework",
    "zf2",
    "html",
    "converter",
    "bbcode"
  ],
  "homepage": "https://github.com/kokspflanze/ZfcBBCode",
  "autoload": {
    "psr-0": {
      "ZfcBBCode\\": "src/",
      "ZfcBBCodeTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Oliver",
      "email": "kokspflanze@gmx.de"
    }
  ],
  "require": {
    "php": ">=5.6",
    "zendframework/zend-servicemanager": "~3.1",
    "zendframework/zend-validator": "~2.8|~3.0",
