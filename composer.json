  "keywords": [
    "framework",
    "zf2",
    "html",
    "converter",
    "bbcode"
  ],
  "homepage": "https://github.com/kokspflanze/ZfcBBCode",
  "autoload": {
    "psr-0": {
      "ZfcBBCode\\": "src/",
      "ZfcBBCodeTest\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Oliver",
      "email": "kokspflanze@gmx.de"
    }
  ],
  "require": {
    "php": ">=5.4",
    "samclarke/sbb-code-parser": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "zendframework/zendframework": "~2.4",
    "fabpot/php-cs-fixer": "~1.0",
    "satooshi/php-coveralls": "dev-master",
    "phpunit/phpcov": "~2.0"
  }
